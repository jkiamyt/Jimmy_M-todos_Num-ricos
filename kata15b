import math
import numpy as np
import matplotlib.pyplot as plt

def f(x,y):
    return 2*x*y 

def euler(y0, x, h, f):
    y = []
    y.append(y0)
    for i in range(1, len(x)):
        y.append(y[i-1] + h*f(x[i-1], y[i-1]))
    return y

n = 11
a = 1
b = 1.15
h = 0.05
x = np.linspace(a,b,n)
y = euler(1,x,h,f)

plt.plot(x,y,'g')

plt.grid()

print('kata 15 b')

print('Valor x: ', x[-1])
print('Valor calculado y: ', y[-1])
